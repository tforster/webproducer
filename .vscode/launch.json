{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "cli.js",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}/tests",
      "program": "${workspaceFolder}/tests/cli.js",
      "runtimeVersion": "14"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "CLI",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/index.js",
      "runtimeVersion": "14",
      "args": [
        "--src",
        "/path/to/src",
        "--dest",
        "/path/to/dest",
        "--data",
        "/path/to/data",
        "--transform",
        "function(){//some func}"
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Commander",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}/tests",
      "program": "${workspaceFolder}/tests/cli.js",
      "runtimeVersion": "14",
      "args": []
    }
  ]
}